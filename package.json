{
  "name": "jenerate",
  "version": "0.0.1",
  "description": "Framework-agnostic, customizable Javascript module generator.",
  "author": "Jake Marsh <jakemmarsh@gmail.com> (http://jakemmarsh.com)",
  "contributors": [
    {
      "name": "J. Ky Marsh",
      "email": "jkymarsh@gmail.com"
    }
  ],
  "bin": {
    "jenerate": "dist/index.js"
  },
  "scripts": {
    "dev": "babel --presets es2015 lib --watch --out-dir dist",
    "test": "./node_modules/.bin/mocha test/**/*.js --compilers js:babel-register",
    "lint": "eslint lib/**/*.js test/**/*.js",
    "build": "babel --presets es2015 lib --out-dir dist"
  },
  "directories": {
    "test": "test"
  },
  "homepage": "https://github.com/jakemmarsh/jenerate",
  "repository": {
    "type": "git",
    "url": "https://github.com/jakemmarsh/jenerate.git"
  },
  "bugs": {
    "url": "https://github.com/jakemmarsh/jenerate/issues"
  },
  "keywords": [
    "javascript",
    "module",
    "generator",
    "js",
    "jenerate"
  ],
  "dependencies": {
    "async": "^1.5.2",
    "yargs": "^4.1.0"
  },
  "preferGlobal": true,
  "private": true,
  "license": "MIT",
  "devDependencies": {
    "babel-cli": "^6.2.0",
    "babel-preset-es2015": "^6.5.0",
    "babel-register": "^6.5.2",
    "chai": "^3.5.0",
    "eslint": "^2.1.0",
    "mocha": "^2.4.5"
  }
}
